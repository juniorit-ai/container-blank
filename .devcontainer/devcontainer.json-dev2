{
  "image": "haicam/juniorit-clang:v1.0.3",
  "hostRequirements": {
    "cpus": 2
  },
  "overrideCommand": false,
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/juniorit/workspace/${localWorkspaceFolderBasename},type=bind,consistency=cached",
  "workspaceFolder": "/home/juniorit/workspace",
  "mounts": [
    "source=/home/frankhe/Dropbox/github_workspace/clang-game-project,target=/home/juniorit/workspace/c-session-1,type=bind,consistency=cached"
  ],
  "containerEnv": {
    "GAMECRAFT_PROJECT_PATH": "/home/juniorit/workspace/${localWorkspaceFolderBasename}"
  },
  "waitFor": "onCreateCommand",
  "updateContentCommand": "",
  "postCreateCommand": "",
  "postAttachCommand": {
    "server": ""
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "JuniorIT-AI.vscode-juniorit",
        "ms-vscode.cpptools-extension-pack",
        "bmewburn.vscode-intelephense-client"
      ],
      "settings": {
        "extensions.ignoreRecommendations": true
      }
    },
    "codespaces": {
      "openFiles": [
        ""
      ]
    }
  },
  "portsAttributes": {
    "9000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    9000
  ]
}